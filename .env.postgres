# PostgreSQL Configuration Example
# Copy this to .env to use PostgreSQL instead of SQLite

# Database Configuration
DATABASE_URL=postgresql://savt_user:savt_password@localhost:5432/savt

# Application Configuration
DEBUG=true
HOST=0.0.0.0
PORT=8000

# Application Settings
APP_NAME=SAVT
VERSION=0.1.0

# Terminology (customize for your use case)
OBJECT_NAME_SINGULAR=pizza
PROPERTY_NAME_SINGULAR=topping

# Security
SECRET_KEY=your-secret-key-change-in-production

# Logging
LOG_TO_FILE=false
